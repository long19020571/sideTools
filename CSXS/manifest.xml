        <UI>
          <Type>Panel</Type>
          <Menu>Side Tools</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>300</Width>
            </Size>
            <MinSize>
              <Height>200</Height>
              <Width>200</Width>
            </MinSize>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./icons/icon.png</Icon>
            <Icon Type="RollOver">./icons/icon.png</Icon>
            <Icon Type="DarkNormal">./icons/icon.png</Icon>
            <Icon Type="DarkRollOver">./icons/icon.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>